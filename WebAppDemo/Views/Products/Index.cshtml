@model IEnumerable<ProductModel>;

<html>
<head>
	<title>Products</title>
</head>
<body>
	<table>
		<tr>
			<td>ID</td>
			<td>Name</td>
			<td>Price</td>
			<td>Date</td>
		</tr>
		@foreach (ProductModel item in Model ?? new List<ProductModel>())
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.Name</td>
				<td>@item.Price</td>
				<td>@item.Dt.ToString("yyyy-MM-dd")</td>
			</tr>
		}
	</table>
</body>
</html>